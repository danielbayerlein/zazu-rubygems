{
  "name": "zazu-rubygems",
  "version": "1.1.1",
  "description": "Zazu plugin to search for gems with rubygems.org",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "lint": "standard --verbose | snazzy",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielbayerlein/zazu-rubygems.git"
  },
  "keywords": [
    "zazu",
    "ruby",
    "rubygems",
    "gem"
  ],
  "author": "Daniel Bayerlein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danielbayerlein/zazu-rubygems/issues"
  },
  "homepage": "https://github.com/danielbayerlein/zazu-rubygems#readme",
  "devDependencies": {
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "snazzy": "^7.0.0",
    "standard": "^10.0.1"
  },
  "dependencies": {
    "cache-conf": "^0.5.0",
    "got": "^6.7.1"
  },
  "jest": {
    "bail": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/*.js"
    ]
  },
  "standard": {
    "env": {
      "jest": true
    }
  }
}
